<Alloy>
	<View id="navBar" class="windowHeader">
		<View id="backBtn" class="backButtonStyle coloredGrey" onClick="back">
			<ImageView image="/images/dark/appbar.arrow.left.png"></ImageView>
		</View>
		<View class="coloredWhite pageTitleStyle">
			<Label id="pageTitle" class="pageTitleStyle">Children's Museum of Houston</Label>
		</View>
		<View id="flyoutBtn" class="menuButtonStyle coloredGrey" onClick="menu">
			<ImageView class="flyoutIcon" image="/images/drawer.png" platform="android"></ImageView>
			<ImageView class="flyoutIcon" image="/images/icons_ios/drawer.png" platform="ios"></ImageView>
		</View>
	</View>
</Alloy>